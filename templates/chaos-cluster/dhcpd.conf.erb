# Viridis arm servers, specific pxe server
class "viridis" {
    match if binary-to-ascii (16, 8, "-", substring (hardware, 0, 4)) = "1-fc-2f-40";
    next-server 10.221.251.14;
    option      domain-name "viridis-cluster.uni.lux";
}



